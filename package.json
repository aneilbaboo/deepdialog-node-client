{
  "name": "deepdialog",
  "version": "0.1.0",
  "description": "Node client for interacting with DeepDialog service",
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/aneilbaboo/deepdialog-node-client.git"
  },
  "keywords": [
    "chatbots",
    "conversationalUI",
    "javascript",
    "AI"
  ],
  "license": "MIT",
  "options": {
    "mocha": "--require scripts/mocha_runner tests/**/*.test.js"
  },
  "scripts": {
    "prepublish": ". ./scripts/prepublish.sh",
    "postinstall": ". ./scripts/postinstall.sh",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "console": "node --require babel-register --require babel-polyfill -e 'var {App, Session, AppServer, Client, Dialog, log} = require(`.`);' -i"
  },
  "directories": {
    "lib": ""
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-eslint": "6.0.0-beta.6",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-env": "1.2.2",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "chai": "3.5.0",
    "dotenv": "2.0.0",
    "eslint": "3.12.2",
    "eslint-mocha": "0.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "4.7.0",
    "mocha": "3.2.0",
    "sinon": "2.3.5"
  },
  "dependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "6.22.1",
    "babel-polyfill": "6.22.0",
    "babel-preset-env": "1.2.2",
    "babel-runtime": "6.x.x",
    "body-parser": "1.16.0",
    "compression": "1.6.2",
    "express": "4.14.0",
    "express-promise-router": "1.1.1",
    "jwt-simple": "0.5.1",
    "lokka": "1.7.0",
    "lokka-transport-http": "1.6.1",
    "micromustache": "5.2.0",
    "morgan": "1.8.1",
    "request": "2.79.0",
    "request-promise": "4.1.1",
    "winston": "2.3.1"
  }
}
