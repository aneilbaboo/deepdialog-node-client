    "chai": "3.x.x",
    "eslint": "3.2.x",
    "eslint-mocha": "0.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "4.7.0",
    "mocha": "2.x.x"
  },
  "dependencies": {
    "babel-core": "6.22.1",
    "babel-eslint": "6.0.0-beta.6",
    "babel-polyfill": "6.22.0",
    "babel-runtime": "6.x.x",
